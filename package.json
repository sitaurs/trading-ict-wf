{
  "name": "trading-analysis-bot",
  "version": "1.1.0",
  "description": "Bot analisis trading otomatis menggunakan Node.js dan WhatsApp.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --max-old-space-size=512 index.js",
    "monitor": "node scripts/monitor.js",
    "test": "node tests/contextManager.test.js && node tests/cronSchedule.test.js && node tests/aggregate.test.js",
    "test:all": "node tests/test_full_narrative_storage.js && node tests/test_stage1_completion.js && node tests/test_profit_monitoring.js",
    "health": "node scripts/api_compatibility_checker.js",
    "cache:summary": "node scripts/cache_manager.js summary",
    "cache:cleanup": "node scripts/cache_manager.js cleanup",
    "cache:stats": "node scripts/cache_manager.js stats",
    "pterodactyl": "node --max-old-space-size=512 --optimize-for-size index.js",
    "lint": "echo 'Add linter configuration as needed'",
    "lint:fix": "echo 'Add linter fix command as needed'"
  },
  "keywords": [
    "trading",
    "bot",
    "whatsapp",
    "nodejs",
    "baileys",
    "gemini"
  ],
  "author": "NandaC",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.7.5",
    "axios": "^1.7.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.11.0",
    "google-spreadsheet": "^4.1.2",
    "node-cron": "^3.0.3",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.32.6",
    "technicalindicators": "^3.0.1",
    "uuid": "^11.1.0"
  }
}
